{# Caso Ajax#}
{% if flashMessage is not defined %}
    <div class="alert alert-danger hidden" id="message-danger" role="alert">
      <div class="container">
        <span id="participante-message-danger"></span>
      </div>
    </div>
{% endif %}


{% for flashMessage in app.session.flashbag.get('mensajeError') %}
  <div class="alert alert-danger" role="alert" id="message-danger">
      <div class="container">{{ flashMessage }}</div>
  </div>
{% endfor %}
